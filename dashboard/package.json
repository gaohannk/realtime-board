{
  "name": "realtime-board",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run build",
    "start": "node -r source-map-support/register ./build/app.js",
    "start:watch": "nodemon",
    "test": "jest -c jest.config.js --detectOpenHandles --coverage",
    "test:watch": "jest --watch -c jest.config.js --detectOpenHandles --coverage",
    "build": "tsc -b && cp -r views build && cp -r public build",
    "clean": "rm -r ../build ./node_modules ./coverage"
  },
  "dependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/http-errors": "^1.6.3",
    "@types/morgan": "^1.7.37",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.17.1",
    "http-errors": "~1.6.3",
    "http-status-codes": "^1.4.0",
    "json-pointer": "^0.6.0",
    "morgan": "~1.9.1",
    "path": "^0.12.7",
    "pug": "2.0.0-beta11",
    "pusher": "^3.0.0",
    "pusher-js": "^5.0.3",
    "source-map-support": "^0.5.16",
    "yayson": "^2.0.8"
  }
}
